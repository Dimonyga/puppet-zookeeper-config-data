{
  "name": "dimonyga-zk_puppet",
  "version": "1.0.5",
  "author": "manos",
  "summary": "This module enables other classes to get of put data directly from and to zookeeper without using hiera",
  "license": "Apache-2.0",
  "source": "https://github.com/Dimonyga/puppet-zookeeper-config-data.git",
  "project_page": "https://github.com/Dimonyga/puppet-zookeeper-config-data",
  "issues_url": "https://github.com/Dimonyga/puppet-zookeeper-config-data/issues",
  "requirements": [
